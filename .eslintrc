{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "import",
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "import/no-unresolved": "error",
    "no-unused-vars": "warn"
  }
}